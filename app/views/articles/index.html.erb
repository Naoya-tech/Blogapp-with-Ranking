  <div id="article_list">
    <% @articles.each do |article| %>
      <div>
        <h2><%= link_to article.title, article unless article.title.nil? %></h2>
        <p><%= article.created_at.to_s(:datatime_jp) %></p>
          <% if user_signed_in? %>
            <p>
              <%= link_to '編集', edit_article_path(article) %>
              <%= link_to '削除', article, method: :delete, data:{ confirm: "削除してよろしいですか？" } %>
            </p>
            <% end %>
      </div>
    <% end %>
  </div>
  <%= render 'sidebar' %>
<div class="clear"></div>

